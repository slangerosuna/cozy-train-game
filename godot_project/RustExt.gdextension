[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../rust_extension/target/debug/librust_extension.so"
linux.release.x86_64 =   "res://../rust_extension/target/release/librust_extension.so"
windows.debug.x86_64 =   "res://../rust_extension/target/debug/rust_extension.dll"
windows.release.x86_64 = "res://../rust_extension/target/release/rust_extension.dll"
macos.debug =            "res://../rust_extension/target/debug/librust_extension.dylib"
macos.release =          "res://../rust_extension/target/release/librust_extension.dylib"
macos.debug.arm64 =      "res://../rust_extension/target/debug/librust_extension.dylib"
macos.release.arm64 =    "res://../rust_extension/target/release/librust_extension.dylib"
